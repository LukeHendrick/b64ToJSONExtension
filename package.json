{
  "name": "b64toString",
  "displayName": "base64-to-string",
  "description": "Converts base64 to an ascii string",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.b64toString",
    "onCommand:extension.stringtoB64"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.b64toString",
        "title": "Base64 Decode"
      },
      {
        "command": "extension.stringtoB64",
        "title": "Base64 Encode"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
