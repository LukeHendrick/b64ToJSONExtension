{
  "name": "b64toJSON",
  "displayName": "base64-to-JSON",
  "publisher": "lukehendrick",
  "description": "Converts base64 to an ascii JSON",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.b64toJSON",
    "onCommand:extension.JSONtoB64"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.b64toJSON",
        "title": "Base64 Decode"
      },
      {
        "command": "extension.JSONtoB64",
        "title": "Base64 Encode"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
